___SANDBOXED_JS_FOR_WEB_TEMPLATE___

// Require the necessary APIs
const logToConsole = require('logToConsole');
const query = require('queryPermission');
const encode = require('encodeUriComponent');
const sendPixel = require('sendPixel');
const makeString = require('makeString');

//declare variables
const product_id = encode(makeString(data.product_id));
const sovReqToken = encode(makeString(data.token));
let baseUrl = '';

//build pixel url
baseUrl = 'https://press-order-api.sovendus.com/ext/' + product_id;
baseUrl += '/image?sovReqToken=';
baseUrl += sovReqToken;

const log = data.debug ? logToConsole : (() => {});
log('Pixel URL: ' + baseUrl);

//send pixel
sendPixel(baseUrl, data.gtmOnSuccess, data.gtmOnFailure);


